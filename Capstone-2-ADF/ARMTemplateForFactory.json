		{
			"name": "[concat(parameters('factoryName'), '/ls_rds_postgreSQL')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "PostgreSqlV2",
				"typeProperties": {
					"server": "[parameters('ls_rds_postgreSQL_properties_typeProperties_server')]",
					"port": 5432,
					"database": "[parameters('ls_rds_postgreSQL_properties_typeProperties_database')]",
					"username": "[parameters('ls_rds_postgreSQL_properties_typeProperties_username')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('ls_rds_postgreSQL_password')]"
					},
					"sslMode": 2,
					"authenticationType": "Basic"
				}
			},
			"dependsOn": []
